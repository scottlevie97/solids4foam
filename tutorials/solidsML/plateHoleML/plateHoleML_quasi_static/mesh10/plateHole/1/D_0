FoamFile
{
    version     2.0;
    format      ascii;
    class       volVectorField;
    location    "1";
    object      D_0;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 0 0 0 0 0];

internalField   uniform (0 0 0);

boundaryField
{
    left
    {
        type            solidSymmetry;
        patchType       symmetryPlane;
        secondOrder     false;
        value           uniform (0 0 0);
    }
    right
    {
        type            solidTraction;
        tractionSeries  
{
        fileName        "$FOAM_CASE/constant/timeVsTraction";
        outOfBounds     clamp;
}
        pressure        uniform 0;
        secondOrder     false;
        setEffectiveTraction false;
        relaxationFactor 1;
        value           uniform (0 0 0);
        gradient        uniform (0 0 0);
    }
    down
    {
        type            solidSymmetry;
        patchType       symmetryPlane;
        secondOrder     false;
        value           uniform (0 0 0);
    }
    up
    {
        type            solidTraction;
        traction        uniform (0 0 0);
        pressure        uniform 0;
        secondOrder     false;
        setEffectiveTraction false;
        relaxationFactor 1;
        value           uniform (0 0 0);
        gradient        uniform (0 0 0);
    }
    hole
    {
        type            solidTraction;
        traction        uniform (0 0 0);
        pressure        uniform 0;
        secondOrder     false;
        setEffectiveTraction false;
        relaxationFactor 1;
        value           uniform (0 0 0);
        gradient        uniform (0 0 0);
    }
    frontAndBack
    {
        type            empty;
    }
}


