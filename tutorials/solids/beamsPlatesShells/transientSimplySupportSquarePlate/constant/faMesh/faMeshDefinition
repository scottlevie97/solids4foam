FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "constant/faMesh";
    object      faMeshDefinition;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

polyMeshPatches  1( front );

boundary
{
    top
    {
        type                patch;
        ownerPolyPatch      front;
        neighbourPolyPatch  top;
    }

    left
    {
        type                patch;
        ownerPolyPatch      front;
        neighbourPolyPatch  left;
    }

    right
    {
        type                patch;
        ownerPolyPatch      front;
        neighbourPolyPatch  right;
    }

    bottom
    {
        type                patch;
        ownerPolyPatch      front;
        neighbourPolyPatch  bottom;
    }
}

