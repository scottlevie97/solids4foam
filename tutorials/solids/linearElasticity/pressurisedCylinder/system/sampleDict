FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    system;
    object      sampleDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

setFormat raw;

surfaceFormat vtk;

interpolationScheme cellPointFace;

fields
(
    sigma:Transformed D
);

sets
(
    line
    {
        type       face;
        axis       distance;
        start (0 0 0.5);
        end   (13.2 13.2 0.5);
    }
);

surfaces
();


