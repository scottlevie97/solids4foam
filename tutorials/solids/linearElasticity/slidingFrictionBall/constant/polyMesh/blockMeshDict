    (1 0 -0.05)
    (-2.2 0.1 -0.05)
    (-0.2 0.1 -0.05)
    (-2.2 0.5 -0.05)
    (-0.2 0.5 -0.05)

    (-1 -0.5 0.05)
    (1 -0.5 0.05)
    (-1 0 0.05)
    (1 0 0.05)
    (-2.2 0.1 0.05)
    (-0.2 0.1 0.05)
    (-2.2 0.5 0.05)
    (-0.2 0.5 0.05)
);

edges
(
    arc 4 5 (-1.2 0 -0.05)
    arc 12 13 (-1.2 0 0.05)
);

blocks
(
    hex 8(0 1 3 2 8 9 11 10) (100 25 1) simpleGrading (1 1 1)
    hex 8(4 5 7 6 12 13 15 14) (100 25 1) simpleGrading (1 1 1)
);

patches
(
    patch topBrickDown
    (
        (4 5 13 12)
    )

    patch topBrickUp
    (
        (6 14 15 7)
    )

    patch bottomBrickDown
    (
        (0 1 9 8)
    )

    patch bottomBrickUp
    (
        (2 10 11 3)
    )

    patch bottomBrickLeft
    (
        (0 8 10 2)
    )

    patch bottomBrickRight
    (
        (1 3 11 9)
    )

    patch topBrickLeft
    (
        (4 12 14 6)
    )

    patch topBrickRight
    (
        (5 7 15 13)
    )

    empty back
    (
        (0 2 3 1)
        (4 6 7 5)
    )

    empty front
    (
        (8 9 11 10)
        (12 13 15 14)
    )
);

