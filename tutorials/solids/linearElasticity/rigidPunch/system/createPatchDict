matchTolerance 1e-3;

pointSync false;

patchInfo
(
    {
        name cylinderTractionFree;
        dictionary
        {
            type patch;
        }
        constructFrom patches;
        patches ("auto0");
    }
    {
        name symmx;
        dictionary
        {
            type symmetryPlane;
        }
        constructFrom patches;
        patches ("auto4");
    }
    {
        name cylinderContact;
        dictionary
        {
            type patch;
        }
        constructFrom patches;
        patches ("auto1");
    }
    {
        name cylinderFixed;
        dictionary
        {
            type patch;
        }
        constructFrom patches;
        patches ("auto2");
    }
    {
        name symmy;
        dictionary
        {
            type symmetryPlane;
        }
        constructFrom patches;
        patches ("auto3");
    }
);

