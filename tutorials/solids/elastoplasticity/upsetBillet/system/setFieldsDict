FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      setFieldsDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

defaultFieldValues
(
    volScalarFieldValue materials 0
);

regions
(
    boxToCell
    {
        box (0 0.00275 0) (0.15 0.003 0.025);

        fieldValues
        (
            volScalarFieldValue materials 1
        );
    }
);

