FoamFile
{
    version     2.0;
    format      binary;
    class       dictionary;
    location    "constant";
    object      mechanicalProperties;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

planeStress     no;

mechanical
(
    copper
    {
        type            neoHookeanElasticMisesPlastic;
        rho             rho [ 1 -3 0 0 0 0 0 ] 8930;
        E               E [ 1 -1 -2 0 0 0 0 ] 117e9;
        nu              nu [ 0 0 0 0 0 0 0 ] 0.35;
        "fileName|file" "$FOAM_CASE/constant/plasticStrainVsYieldStress.taylor1981";
        outOfBounds     clamp;
    }
);

