FoamFile
{
    version     2.0;
    format      ascii;
    class       volVectorField;
    location    "0";
    object      D;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 0 0 0 0 0];

internalField   uniform (0 0 0);

boundaryField
{
    left
    {
        type            calculated;
        value           uniform (0 0 0);
    }
    dieContact
    {
        type            calculated;
        value           uniform (0 0 0);
    }
    billetContact
    {
        type            calculated;
        value           uniform (0 0 0);
    }
    loading
    {
        type            calculated;
        value           uniform (0 0 0);
    }
    back
    {
        type            solidWedge;
        patchType       wedge;
        value           uniform (0 0 0);
    }
    front
    {
        type            solidWedge;
        patchType       wedge;
        value           uniform (0 0 0);
    }
    axis
    {
        type            empty;
    }
}

